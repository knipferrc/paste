{
  "name": "pastey",
  "version": "0.1.0",
  "description": "Simple and elegant pasting app",
  "author": "Tyler Knipfer <knipferrc@gmail.com>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/knipferrc/pastey.git"
  },
  "bugs": {
    "url": "https://github.com/knipferrc/pastey/issues"
  },
  "keywords": [
    "next.js",
    "react",
    "graphql",
    "pastey",
    "apollo"
  ],
  "now": {
    "alias": "pastey"
  },
  "scripts": {
    "dev": "node server",
    "build": "next build",
    "start": "NODE_ENV=production node server",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"{,!(node_modules|.next|static)/**/}*.js\"",
    "deploy": "now --docker && now alias"
  },
  "dependencies": {
    "antd": "^2.13.6",
    "apollo-server-express": "^1.1.7",
    "babel-eslint": "^8.0.1",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-inline-dotenv": "^1.1.1",
    "babel-plugin-inline-import-graphql-ast": "^2.0.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "bcrypt": "^1.0.3",
    "better-sqlite3": "^4.0.3",
    "compression": "^1.7.1",
    "cookie": "^0.3.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.5.1",
    "helmet": "^3.9.0",
    "hpp": "^0.2.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.4",
    "jsonwebtoken": "^8.1.0",
    "next": "^4.1.0",
    "prettier": "^1.7.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.0.0",
    "uuid": "^3.1.0"
  }
}
