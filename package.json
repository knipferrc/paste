{
  "name": "pastey",
  "version": "0.1.0",
  "description": "Simple and elegant pasting app",
  "author": "Tyler Knipfer <knipferrc@gmail.com>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/knipferrc/pastey.git"
  },
  "bugs": {
    "url": "https://github.com/knipferrc/pastey/issues"
  },
  "keywords": [
    "razzle",
    "react",
    "graphql",
    "pastey",
    "apollo"
  ],
  "now": {
    "alias": "pastey",
    "dotenv": true
  },
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "deploy": "now --docker && now alias"
  },
  "dependencies": {
    "apollo-server-express": "^1.1.7",
    "babel-eslint": "^8.0.1",
    "babel-plugin-styled-components": "^1.2.1",
    "bcrypt": "^1.0.3",
    "better-sqlite3": "^4.0.3",
    "bulma": "^0.6.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "formik": "^0.9.4",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.6.1",
    "helmet": "^3.9.0",
    "hpp": "^0.2.2",
    "html-minifier": "^3.5.6",
    "js-cookie": "^2.1.4",
    "jsonwebtoken": "^8.1.0",
    "prettier": "^1.7.4",
    "raf": "^3.4.0",
    "razzle": "^0.8.4",
    "react": "^16.0.0",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.0.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "styled-components": "^2.2.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "uuid": "^3.1.0"
  }
}
