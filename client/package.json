{
  "name": "pastey-client",
  "version": "0.0.1",
  "private": true,
  "description": "Client for pastey",
  "repository": "https://github.com/knipferrc/pastey.git",
  "author": "knipferrc@gmail.com (Tyler Knipfer)",
  "license": "GPL-3.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && react-snapshot",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "deploy": "now && now alias",
    "now-start": "pushstate-server build"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "prettier": "^1.6.1",
    "prop-types": "^15.5.10",
    "pushstate-server": "^3.0.1",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13",
    "react-snapshot": "^1.2.0",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "reselect": "^3.0.1",
    "semantic-ui-react": "^0.73.0",
    "slate": "^0.24.0",
    "slate-react": "^0.1.0",
    "styled-components": "^2.1.2"
  },
  "now": {
    "alias": "pastey"
  },
  "reactSnapshot": {
    "include": [
      "/",
      "/login",
      "/signup",
      "/blogs",
      "/create"
    ]
  },
  "proxy": "http://localhost:5000"
}
